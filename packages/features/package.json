{
  "name": "@app/features",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./limeroad/screens/*": "./src/limeroad/screens/*.tsx",
    "./limeroad/providers/*": "./src/limeroad/providers/*.tsx"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@app/core": "workspace:*",
    "@app/services": "workspace:*",
    "@app/ui": "workspace:*",
    "@tanstack/react-query": "catalog:",
    "@tanstack/react-query-devtools": "^5.76.1"
  },
  "devDependencies": {
    "@app/eslint-config": "workspace:*",
    "@app/prettier-config": "workspace:*",
    "@app/tsconfig": "workspace:*",
    "@app/types": "workspace:*",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "react-native": "catalog:react19"
  },
  "prettier": "@app/prettier-config"
}
